  "bugs": {
    "url": "https://github.com/TheCharlatan/bitbox02_backup/issues"
  },
  "homepage": "https://github.com/TheCharlatan/bitbox02_backup#readme",
  "dependencies": {
    "bip39": "3.1.0",
    "browserify": "17.0.0",
    "protobufjs": "7.2.4",
    "protobufjs-cli": "1.1.1"
  }
}
